{
    "files.associations": {
        "stdio.h": "c",
        "math.h": "c",
        "windows.h": "c",
        "string.h": "c"
    },
    
    "files.exclude": {
        // 确保这一行是 "**/.*" 开头，且前面没有 //
        "**/*.exe": true, 
        
